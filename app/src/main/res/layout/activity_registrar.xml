<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#363434"
    tools:context=".Registrar">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="130dp"
        android:layout_height="20dp"
        android:text="Alredy a member?"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="617dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*********************************************"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="582dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="210dp"
        android:layout_height="43dp"
        android:layout_marginStart="84dp"
        android:layout_marginLeft="84dp"
        android:layout_marginBottom="48dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/b_register"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="210dp"
        android:layout_height="43dp"
        android:layout_marginStart="84dp"
        android:layout_marginLeft="84dp"
        android:layout_marginBottom="172dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/b_register"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="210dp"
        android:layout_height="43dp"
        android:layout_marginStart="84dp"
        android:layout_marginLeft="84dp"
        android:layout_marginBottom="300dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/b_register"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/b_register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#DDFF4444"
        android:onClick="register"
        android:text="Register"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="508dp" />

    <Button
        android:id="@+id/b_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="652dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="USERNAME"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#ECE4E4"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="PASSWORD"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#ECE4E4"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="250dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="EMAIL"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#ECE4E4"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="374dp" />
</androidx.constraintlayout.widget.ConstraintLayout>